class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        int n = nums.size();
        int count =  0;
        unordered_map<int,int>m;
        m[0] = 1;
        int preSum = 0;

        for(int i=0;i<n;i++){
            preSum += nums[i];
            int remove = preSum - k;
            count += m[remove];
            m[preSum]++;
        }
        
        return count;
    }
};
