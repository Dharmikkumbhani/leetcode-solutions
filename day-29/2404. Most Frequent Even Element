class Solution {
public:
    int mostFrequentEven(vector<int>& nums) {
        map<int,int>m;
        int n = nums.size();

        for(int i=0;i<n;i++){
            if(nums[i] % 2 == 0){
                m[nums[i]]++;
            }
        }

        int mini = -1;
        int minEle = -1;
        for(auto pair:m){
            if(pair.second > mini){
                mini = pair.second;
                minEle = pair.first;
            }
        }
        return minEle;
    }
};
