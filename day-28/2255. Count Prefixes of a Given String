class Solution {
public:
    int countPrefixes(vector<string>& words, string s) {
        int count = 0;
        for(int i=0;i<words.size();i++){
            int j = 0;
            bool flag = true;
            while(j < words[i].size() && j < s.length()){
                if(words[i][j] != s[j]){
                    flag = false;
                    break;
                }
                j++;
            }
            if(flag && j >= words[i].size()){
                count++;
            }
        }   
        return count;
    }
};
